[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust/target/debug/tiny_console.so"
linux.release.x86_64 =   "res://rust/target/release/tiny_console.so"
windows.debug.x86_64 =   "res://rust/target/debug/tiny_console.dll"
windows.release.x86_64 = "res://rust/target/release/tiny_console.dll"
macos.debug =            "res://rust/target/debug/tiny_console.dylib"
macos.release =          "res://rust/target/release/tiny_console.dylib"
macos.debug.arm64 =      "res://rust/target/debug/tiny_console.dylib"
macos.release.arm64 =    "res://rust/target/release/tiny_console.dylib"

[icons]
